#!/usr/bin/expect

set SSH /home/himsen/Projects/openssh-portable-intermac/ssh
set HOST localhost
set PORT 22222
set ID id_rsa_im
set ID_LOCATION .
set AUTHCIPHER aes128-gcm@openssh.com

spawn $SSH -c $AUTHCIPHER -o Compression=no $HOST -p $PORT -i $ID_LOCATION/$ID
send "l"
sleep .5
send "s"
sleep .5
send "\r"
interact
